<?xml version='1.0' encoding='ISO-8859-1'?>
<fix8>
	<session name="DLD1"
				role="initiator"
				fix_version="4400"
				active="true"
				ip="127.0.0.1"
				port="11001"
				sender_comp_id="DLD_TEX"
				target_comp_id="TEX_DLD"
				session_log="session_log"
				protocol_log="protocol_log"
				login_retry_interval="15000"
				login_retries="100"
				persist="bdb0" />

	<persist name="bdb0"
				type="bdb" dir="./run"
				db="myfix_client.db" />

	<persist name="mem0"
				type="mem" />

	<log 		name="session_log"
				type="session"
				filename="|/bin/cat"
				rotation="5"
				flags="timestamp|sequence|compress|thread"/>

	<log 		name="protocol_log"
				type="protocol"
				filename="./run/myfix_client_protocol.log"
				rotation="5"
				flags="sequence|append|direction"/>
</fix8>
